<div class="row">
  <div class="col-md-12">
    <table class="table table-responsive table-bordered table-hover table-striped">
      <thead>
        <tr>
          <th>Title</th>
          <th>Type</th>
          <th>Owner</th>
        </tr>
      </thead>
      <tbody>
        {{#each model.results as |item|}}
          {{results-item model=item}}
        {{/each}}
      </tbody>
    </table>
    {{item-list-pager model=model num=num}}
  </div>
</div>
